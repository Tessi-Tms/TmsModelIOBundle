<form name="tms_model_io-import" action="{{ action }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <button type="submit" class="btn btn-default">{% trans %}Valid{% endtrans %}</button>
</form>

<script>
    $(document).ready(function() {
        $('form[name="tms_model_io-import"]').find('button[type="submit"]').on('click', function() {
            if ($('.tmsmodelio_removal-warning').is(':checked')) {
                if (!confirm('{% trans %}All the existing entries will be deleted. Confirm to proceed.{% endtrans %}')) {
                    return false;
                }
            }

            return true;
        });
    });
</script>
